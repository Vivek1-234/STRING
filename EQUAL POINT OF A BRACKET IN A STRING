#include<iostream>

using namespace std;
 
int findIndex(string s ){
  
  int i =0  ; int n = s.size() ; int ans = 0 ; 
  while(i<n ){
	  if(s[i] == ')'){

		  ans = ans +1;
		  i++;
	  }
	  else{
		  i++;

	  }

  }
  return ans ; 
}

int main(){

string s   = "(()))(()()())))";
cout<<findIndex(s);

return 0;
 }

 
